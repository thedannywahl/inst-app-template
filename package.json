{
  "name": "inst-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run --convention=nextjs -- next dev --turbo",
    "build": "dotenvx run --convention=nextjs -- next build",
    "start": "dotenvx run --convention=nextjs -- next start",
    "biome-apply": "pnpm lint && pnpm check && pnpm format",
    "lint": "biome lint ./",
    "check": "biome check --apply ./",
    "format": "biome format --write ./",
    "rage": "biome rage",
    "test": "playwright test",
    "test-ct": "playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.38.0",
    "@instructure/emotion": "^9.0.1",
    "@instructure/ui": "^9.0.1",
    "@instructure/ui-react-utils": "^9.0.1",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.3",
    "@types/mdx": "^2.0.13",
    "ansis": "^3.2.0",
    "browserslist": "^4.23.0",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "next-intl": "^3.13.0",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@instructure/browserslist-config-canvas-lms": "^2.0.0",
    "@microsoft/api-documenter": "^7.24.5",
    "@microsoft/api-extractor": "^7.43.4",
    "@playwright/experimental-ct-react": "^1.44.0",
    "@playwright/test": "^1.44.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "=9.1.1"
  },
  "packageManager": "pnpm@9.1.1+sha256.9551e803dcb7a1839fdf5416153a844060c7bce013218ce823410532504ac10b",
  "pnpm": {
    "overrides": {
      "@instructure/ui-test-sandbox>sinon": "^17.0.1"
    }
  },
  "browserslist": ["extends @instructure/browserslist-config-canvas-lms"]
}
